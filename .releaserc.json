{
    "plugins": [
        "semantic-release-gitmoji",
        "@semantic-release/github",
        "@semantic-release/npm",
        [
            "@semantic-release/git",
            {
                "assets": [
                    "package.json",
                    "package-lock.json"
                ],
                "message": ":bookmark: v${nextRelease.version} [skip ci]\n\nhttps://github.com/SPGoding/datapack-json-schemas/releases/tag/${nextRelease.gitTag}"
            }
        ]
    ]
}